	shmem: shmem@F0000000 {
		compatible = "samsung,exynos8890-shm_ipc";

		reg = <0x0 0xF0000000 0x8800000>;

		shmem,ipc_offset = <0x8000000>;
		shmem,ipc_size = <0x800000>;
	};
};
