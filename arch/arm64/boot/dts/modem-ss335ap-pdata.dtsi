				iod,io_type = <0>;
				iod,links = <0x200>;
				iod,attrs = <0x82>;
				iod,app = "RFS";
				iod,ul_num_buffers = <16>;
				iod,ul_buffer_size = <2048>;
				iod,dl_num_buffers = <512>;
				iod,dl_buffer_size = <2048>;
			};
			io_device_3 {
				iod,name = "umts_csd";
