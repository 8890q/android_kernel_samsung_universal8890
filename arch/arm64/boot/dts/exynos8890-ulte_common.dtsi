			net_boost,table_size = <1>;
			net_boost,table= <64 0 0 1300000 0 1264000 400000 0 0 0>;
		};
		boot_device@3 {
			net_boost,label="P2P";
			net_boost,node="p2p-wlan0-0";
			net_boost,table_size = <3>;
			net_boost,table= <
