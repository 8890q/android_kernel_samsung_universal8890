	hsi2c@13670000 {
		status = "okay";
		sec-nfc@27 {
			compatible = "sec-nfc";
			reg = <0x27>;

			interrupts = <2 0 0>;
			interrupt-parent = <&gpa1>;

			sec-nfc,ven-gpio = <&gpi2 7 0>;
			sec-nfc,irq-gpio = <&gpa1 2 0>;
			sec-nfc,firm-gpio = <&gpe7 0 0>;
			sec-nfc,clk_req-gpio = <&gpa2 7 0xf>;
			sec-nfc,clk_use = <2>;
			sec-nfc,nfc_pvdd = "VDD_NFC_1P8";
			pinctrl-names = "default";
			pinctrl-0 = <&nfc_n5_en &nfc_n5_firm>;

			clocks = <&clock 1000>;
			clock-names = "oscclk_nfc";
			sec-nfc,nfc_ap_clk;

			sec-nfc,ldo_control;
		};

		adpd143@64 {
			compatible = "adpd143";
			reg = <0x64>;
			interrupt-parent = <&gpa1>;
			interrupts = <7 0 0>;
			pinctrl-names = "default", "sleep", "idle";
			pinctrl-0 = <&hrm_irq>;
			pinctrl-1 = <&hrm_irqsleep>;
			pinctrl-2 = <&hrm_irqidle>;
			adpd143,irq_gpio = <&gpa1 7 0>;
			adpd143,vdd_1p8 = "HRM_1.8V_AP";
			adpd143,led_3p3 = "HRM_3.3V_AP";
			#adpd143,i2c_1p8= "VDD_NFC_1P8";
			adpd143,dual-hrm = <0>;
